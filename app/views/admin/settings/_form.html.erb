<%=render 'shared/error_messages',:target=>@setting%>




<%=simple_form_for([:admin,@setting],:html=>{:class=>"form-horizontal"}) do |f|%>

	<%= f.input :name,:input_html=>{:class=>"input-xxlarge"}%>
	<%= f.input :key,:input_html=>{:class=>"input-xxlarge"}%>
	<%= f.input :value,:as=>:text,:input_html=>{:class=>"input-xxlarge"}%>

	


	<div class="form-actions">
		<button type="submit" class="btn btn-primary">保存</button> 
	</div> 
<%end%>